{
  "name": "zheng",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "setup": "pnpm install",
    "dev": "vite",
    "serve": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "jest --coverage",
    "lint:format": "prettier -c ./",
    "lint:format:fix": "prettier -w ./",
    "lint:js": "eslint . --ext .js --ext .ts --ext .jsx --ext .tsx --no-fix",
    "lint:js:fix": "eslint . --ext .js --ext .ts --ext .jsx --ext .tsx --fix",
    "lint:style": "stylelint **/*.scss",
    "lint:style:fix": "stylelint  **/*.scss --fix",
    "lint:commit": "commitlint --from HEAD~1 --to HEAD --verbose",
    "inspect": "pnpm run lint:commit && pnpm run lint:format && pnpm run lint:js && pnpm run lint:style && pnpm run type-check",
    "ci:test": "jest --coverage",
    "ci:lint": "pnpm lint",
    "ci:build": "spartan-cli-service ci:build  --monorepo=false --project=zheng",
    "ci:deploy": "spartan-cli-service ci:static --entryKey=zheng --env=$CI_ENVIRONMENT_NAME"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@antv/g6": "^4.7.12",
    "@h/h-rest": "latest",
    "@h/h-toolbox": "latest",
    "@h/sass": "latest",
    "@sentry/react": "^7.7.0",
    "@sentry/tracing": "^7.7.0",
    "antd": "^4.24.1",
    "classnames": "^2.3.2",
    "mobx": "^6.6.1",
    "mobx-react": "^7.5.2",
    "moment": "^2.29.4",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@h/fe-commitlint": "^0.2.0",
    "@h/fe-lint": "2.0.0-alpha.0",
    "@spartan/cli-plugin-ci": "^2.1.1",
    "@spartan/cli-service": "^2.0.2",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "^8.20.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4",
    "vite": "^3.0.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.19.3"
  }
}
